<?xml version="1.0" encoding="UTF-8"?>
<configuration version="2.0" label="Gradle Profiling Java 9" description="Profiling settings optimized for insights into Gradle builds" provider="Oracle">

  <event name="com.oracle.jdk.ThreadAllocationStatistics">
    <setting name="enabled">true</setting>
    <setting name="period">everyChunk</setting>
  </event>

  <event name="com.oracle.jdk.ClassLoadingStatistics">
    <setting name="enabled">true</setting>
    <setting name="period">1000 ms</setting>
  </event>

  <event name="com.oracle.jdk.ClassLoaderStatistics">
    <setting name="enabled">true</setting>
    <setting name="period">everyChunk</setting>
  </event>

  <event name="com.oracle.jdk.JavaThreadStatistics">
    <setting name="enabled">true</setting>
    <setting name="period">1000 ms</setting>
  </event>

  <event name="com.oracle.jdk.ThreadStart">
    <setting name="enabled">true</setting>
  </event>

  <event name="com.oracle.jdk.ThreadEnd">
    <setting name="enabled">true</setting>
  </event>

  <event name="com.oracle.jdk.ThreadSleep">
    <setting name="enabled">true</setting>
    <setting name="stackTrace">true</setting>
    <setting name="threshold" control="synchronization-threshold">1 ms</setting>
  </event>

  <event name="com.oracle.jdk.ThreadPark">
    <setting name="enabled">true</setting>
    <setting name="stackTrace">true</setting>
    <setting name="threshold" control="synchronization-threshold">1 ms</setting>
  </event>

  <event name="com.oracle.jdk.JavaMonitorEnter">
    <setting name="enabled">true</setting>
    <setting name="stackTrace">true</setting>
    <setting name="threshold" control="synchronization-threshold">1 ms</setting>
  </event>

  <event name="com.oracle.jdk.JavaMonitorWait">
    <setting name="enabled">true</setting>
    <setting name="stackTrace">true</setting>
    <setting name="threshold" control="synchronization-threshold">1 ms</setting>
  </event>

  <event name="com.oracle.jdk.JavaMonitorInflate">
    <setting name="enabled">true</setting>
    <setting name="stackTrace">true</setting>
    <setting name="threshold" control="synchronization-threshold">1 ms</setting>
  </event>

  <event name="com.oracle.jdk.ReservedStackActivation">
    <setting name="enabled">true</setting>
    <setting name="stackTrace">true</setting>
  </event>

  <event name="com.oracle.jdk.ClassLoad">
    <setting name="enabled" control="class-loading-enabled">false</setting>
    <setting name="stackTrace">true</setting>
    <setting name="threshold">0 ms</setting>
  </event>

  <event name="com.oracle.jdk.ClassDefine">
    <setting name="enabled" control="class-loading-enabled">false</setting>
    <setting name="stackTrace">true</setting>
  </event>

  <event name="com.oracle.jdk.ClassUnload">
    <setting name="enabled" control="class-loading-enabled">false</setting>
  </event>

  <event name="com.oracle.jdk.JVMInformation">
    <setting name="enabled">true</setting>
    <setting name="period">beginChunk</setting>
  </event>

  <event name="com.oracle.jdk.InitialSystemProperty">
    <setting name="enabled">true</setting>
    <setting name="period">beginChunk</setting>
  </event>

  <event name="com.oracle.jdk.ExecutionSample">
    <setting name="enabled" control="method-sampling-enabled">true</setting>
    <setting name="period" control="method-sampling-interval">10 ms</setting>
  </event>

  <event name="com.oracle.jdk.SafepointBegin">
    <setting name="enabled">true</setting>
    <setting name="threshold">0 ms</setting>
  </event>

  <event name="com.oracle.jdk.SafepointStateSynchronization">
    <setting name="enabled">false</setting>
    <setting name="threshold">0 ms</setting>
  </event>

  <event name="com.oracle.jdk.SafepointWaitBlocked">
    <setting name="enabled">false</setting>
    <setting name="threshold">0 ms</setting>
  </event>

  <event name="com.oracle.jdk.SafepointCleanup">
    <setting name="enabled">false</setting>
    <setting name="threshold">0 ms</setting>
  </event>

  <event name="com.oracle.jdk.SafepointCleanupTask">
    <setting name="enabled">false</setting>
    <setting name="threshold">0 ms</setting>
  </event>

  <event name="com.oracle.jdk.SafepointEnd">
    <setting name="enabled">false</setting>
    <setting name="threshold">0 ms</setting>
  </event>

  <event name="com.oracle.jdk.ExecuteVMOperation">
    <setting name="enabled">true</setting>
    <setting name="threshold">0 ms</setting>
  </event>

  <event name="com.oracle.jdk.ThreadDump">
    <setting name="enabled" control="thread-dump-enabled">true</setting>
    <setting name="period" control="thread-dump-interval">60 s</setting>
  </event>

  <event name="com.oracle.jdk.IntFlag">
    <setting name="enabled">true</setting>
    <setting name="period">beginChunk</setting>
  </event>

  <event name="com.oracle.jdk.UnsignedIntFlag">
    <setting name="enabled">true</setting>
    <setting name="period">beginChunk</setting>
  </event>

  <event name="com.oracle.jdk.LongFlag">
    <setting name="enabled">true</setting>
    <setting name="period">beginChunk</setting>
  </event>

  <event name="com.oracle.jdk.UnsignedLongFlag">
    <setting name="enabled">true</setting>
    <setting name="period">beginChunk</setting>
  </event>

  <event name="com.oracle.jdk.DoubleFlag">
    <setting name="enabled">true</setting>
    <setting name="period">beginChunk</setting>
  </event>

  <event name="com.oracle.jdk.BooleanFlag">
    <setting name="enabled">true</setting>
    <setting name="period">beginChunk</setting>
  </event>

  <event name="com.oracle.jdk.StringFlag">
    <setting name="enabled">true</setting>
    <setting name="period">beginChunk</setting>
  </event>

  <event name="com.oracle.jdk.IntFlagChanged">
    <setting name="enabled">true</setting>
  </event>

  <event name="com.oracle.jdk.UnsignedIntFlagChanged">
    <setting name="enabled">true</setting>
  </event>

  <event name="com.oracle.jdk.LongFlagChanged">
    <setting name="enabled">true</setting>
  </event>

  <event name="com.oracle.jdk.UnsignedLongFlagChanged">
    <setting name="enabled">true</setting>
  </event>

  <event name="com.oracle.jdk.DoubleFlagChanged">
    <setting name="enabled">true</setting>
  </event>

  <event name="com.oracle.jdk.BooleanFlagChanged">
    <setting name="enabled">true</setting>
  </event>

  <event name="com.oracle.jdk.StringFlagChanged">
    <setting name="enabled">true</setting>
  </event>

  <event name="com.oracle.jdk.ObjectCount">
    <setting name="enabled" control="memory-profiling-enabled-all">false</setting>
    <setting name="period">everyChunk</setting>
  </event>

  <event name="com.oracle.jdk.GCConfiguration">
    <setting name="enabled" control="gc-enabled-normal">true</setting>
    <setting name="period">everyChunk</setting>
  </event>

  <event name="com.oracle.jdk.GCHeapConfiguration">
    <setting name="enabled" control="gc-enabled-normal">true</setting>
